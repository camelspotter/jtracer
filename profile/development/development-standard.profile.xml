<profile>
    <id>development-standard</id>

    <activation>
        <activeByDefault>true</activeByDefault>
    </activation>

    <properties>
        <!-- Generic -->
        <build.phase>development</build.phase>

        <!-- Log4j log levels (for third party) -->
        <log4j.thirdParty.level.full>trace</log4j.thirdParty.level.full>
        <log4j.thirdParty.level.main>trace</log4j.thirdParty.level.main>
        <log4j.thirdParty.level.console>trace</log4j.thirdParty.level.console>

        <!-- Log4j2 log levels (for jTracer) -->
        <log4j.level.full>trace</log4j.level.full>
        <log4j.level.main>trace</log4j.level.main>
        <log4j.level.console>trace</log4j.level.console>
    </properties>
</profile>
